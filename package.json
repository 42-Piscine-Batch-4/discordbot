{
  "name": "piscine-4-discordbot",
  "version": "1.1.2",
  "description": "More description to come!",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --minify",
    "start": "node dist/index.js",
    "format": "prettier . --write",
    "prod": "npm ci && npm run build && npm run start",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "prettier . --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/42-Piscine-Batch-4/discordbot.git"
  },
  "keywords": [
    "Discord",
    "Piscine",
    "42",
    "Student"
  ],
  "author": "Piscine 4",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/42-Piscine-Batch-4/discordbot/issues"
  },
  "homepage": "https://github.com/42-Piscine-Batch-4/discordbot#readme",
  "dependencies": {
    "@types/lodash": "^4.17.0",
    "a-calc": "^1.3.12",
    "axios": "^1.6.8",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "night-api": "^1.3.0"
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "tsup": "^8.0.2",
    "tsx": "^4.8.2",
    "typescript": "^5.4.5"
  }
}
